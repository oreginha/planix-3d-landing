{
  "name": "planix-3d-landing",
  "version": "1.0.0",
  "description": "Planix - Landing page 3D moderna con React Three Fiber y TypeScript",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:railway": "tsc --skipLibCheck && vite build --config vite.railway.config.ts",
    "preview": "vite preview --host 0.0.0.0 --port $PORT",
    "start": "node server.js",
    "start:serve": "serve -s dist -l tcp://0.0.0.0:$PORT",
    "serve": "serve -s dist -l ${PORT:-4173}",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@react-three/drei": "9.122.0",
    "@react-three/fiber": "^8.15.12",
    "@react-three/postprocessing": "3.0.4",
    "@react-three/rapier": "^1.3.0",
    "express": "^4.18.2",
    "framer-motion": "10.18.0",
    "gsap": "3.13.0",
    "leva": "^0.9.35",
    "lucide-react": "^0.294.0",
    "meshline": "^3.1.6",
    "postprocessing": "6.37.4",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "serve": "^14.2.1",
    "three": "^0.158.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.3.7",
    "@types/three": "^0.158.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "terser": "5.40.0",
    "typescript": "^5.3.3",
    "vite": "^4.5.0"
  }
}
